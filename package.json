{
  "name": "chtoolbox",
  "version": "1.0.0",
  "description": "Toolbox of utilities for the Community Health Toolkit",
  "keywords": [
    "cht",
    "community health toolkit",
    "medic",
    "toolbox"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib.d.mts",
      "import": "./dist/lib.mjs",
      "require": "./dist/lib.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkuester/chtoolbox.git"
  },
  "bin": {
    "chtx": "bin/index.js"
  },
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "prepublishOnly": "npm pkg set files='[\"dist\"]' --json && npm remove tsx && tsdown",
    "lint": "tsgo -p tsconfig.json && eslint --color --cache . && knip --production",
    "test": "nyc mocha \"test/**/*\""
  },
  "license": "AGPL-3.0-only",
  "release": {
    "branches": "main",
    "tagFormat": "${version}"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@istanbuljs/esm-loader-hook": "^0.3.0",
    "@medic/eslint-config": "^1.2.0",
    "@octokit/core": "^7.0.5",
    "@tsconfig/node20": "^20.1.6",
    "@tsconfig/strictest": "^2.0.7",
    "@types/chai": "^5.2.3",
    "@types/deep-equal-in-any-order": "^1.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.24",
    "@types/pouchdb-adapter-http": "^6.1.6",
    "@types/pouchdb-core": "^7.0.15",
    "@types/pouchdb-mapreduce": "^6.1.10",
    "@types/semver": "^7.7.1",
    "@types/sinon": "^21.0.0",
    "@types/sinon-chai": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@typescript/native-preview": "^7.0.0-dev.20251030.1",
    "chai": "^6.2.0",
    "chai-exclude": "^3.0.1",
    "deep-equal-in-any-order": "^2.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "esmock": "^2.7.3",
    "globals": "^16.4.0",
    "knip": "^5.66.4",
    "mocha": "11.6.0",
    "nyc": "^17.1.0",
    "semantic-release": "^25.0.1",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.1",
    "tsdown": "^0.19.0-beta.5",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "@effect/cli": "^0.71.0",
    "@effect/platform": "^0.94.5",
    "@effect/platform-node": "^0.98.4",
    "@octokit/plugin-paginate-rest": "^14.0.0",
    "@octokit/rest": "^22.0.0",
    "effect": "^3.18.4",
    "env-paths": "^3.0.0",
    "get-port": "^7.1.0",
    "pouchdb-adapter-http": "^9.0.0",
    "pouchdb-core": "^9.0.0",
    "pouchdb-mapreduce": "^9.0.0",
    "semver": "^7.7.3",
    "test-data-generator": "github:medic/test-data-generator#chtoolbox",
    "tsx": "^4.20.6",
    "uuid": "^13.0.0"
  },
  "overrides": {
    "whatwg-url": "^14.1.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/commands/**/*",
      "src/index.ts",
      "src/services/test-data-generator.ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "report-dir": "./.nyc_output/reports",
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  }
}
